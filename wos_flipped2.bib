@article{ WOS:000581106400002,
Author = {Islas Torres, Claudia and Carranza Alcantar, Maria del Rocio},
Title = {Content analysis of formative experience through an flipped classroom},
Journal = {REVISTA VIRTUAL UNIVERSIDAD CATOLICA DEL NORTE},
Year = {2020},
Volume = {61},
Pages = {3-18},
Month = {SEP-DEC},
Abstract = {Starting from the premise of investing moments and roles of traditional
   teaching for the innovation of the teaching-learning process, flipped
   classroom was implemented in a postgraduate course. The objective was:
   to analyze the experience of students regarding the implementation of
   the inverted classroom. The qualitative approach research used the
   content analysis technique based on the written argument of 13 students
   who presented an essay about their training experience at the end of the
   semester; in addition, the observation record of the teacher responsible
   for the subject was considered. The main findings revealed the initial
   need to adapt to the model, to later make it part of the dynamics of its
   training process, being an option that made the environment in which the
   class developed more flexible. It was a model accepted by the students,
   even with certain reservations, and in addition to identifying the need
   for adaptation to it, the results lead to infer that the literature is
   not overwhelming regarding its impact, in terms of adjustment and
   acceptance of its implications for teaching and learning, so it is
   necessary to use different methodological approaches to obtain empirical
   data that show valid testimonies and talk about the model.},
