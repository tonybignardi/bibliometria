@article{ WOS:000455949400011,
Author = {Salas-Rueda, Ricardo-Adan and Lugo-Garcia, Jose-Luis},
Title = {Impact of the flipped classroom during the higher education process on
   the derivatives considering the science of data and learning machine},
Journal = {EDMETIC},
Year = {2019},
Volume = {8},
Number = {1},
Pages = {147-170},
Abstract = {This quantitative research aims to analyze the incorporation of the
   flipped classroom in the teaching-learning process on mathematics. The
   sample consists of 88 students of the Business School who studied the
   subject Basic Mathematics for Business during the 2016 school year. This
   study analyzes the impact of the flipped classroom for the compression,
   skill, application and utility of the derivatives. By means of
   regression, these variables are used for the construction of four
   forecast models related to the grade of the partial exam. The results of
   learning machine with 50\%, 60\% and 70\% of training indicate that the
   flipped classroom is an innovative, creative and ideal model to
   facilitate the teaching-learning process on mathematics. Likewise, the
   science of data allows establishing different predictive models on the
   use of the flipped classroom in the educational field by means of the
   decision tree technique. Finally, teachers can modify and update school
   activities through the flipped classroom.},
