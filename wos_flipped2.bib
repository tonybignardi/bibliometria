@article{ WOS:000655315200006,
Author = {Chen, Yuh-Tyng and Liou, Shyhnan and Chen, Sheau-Ming},
Title = {Flipping the procedural knowledge learning - a case study of software
   learning},
Journal = {INTERACTIVE LEARNING ENVIRONMENTS},
