@article{ WOS:000427499500010,
Author = {Lopes, Ana Paula and Soares, Filomena},
Title = {Perception and performance in a flipped Financial Mathematics classroom},
Journal = {INTERNATIONAL JOURNAL OF MANAGEMENT EDUCATION},
Year = {2018},
