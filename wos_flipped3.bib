@article{ WOS:000639310500013,
Author = {Martinelli, Susan M. and Chen, Fei and Isaak, Robert S. and Huffmyer,
   Julie L. and Neves, Sara E. and Mitchell, John D.},
Title = {Educating Anesthesiologists During the Coronavirus Disease 2019 Pandemic
   and Beyond},
Journal = {ANESTHESIA AND ANALGESIA},
