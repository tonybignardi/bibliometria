@article{ WOS:000691884000002,
Author = {Kwon, Oh Young},
Title = {Flipped learning: an alternative pedagogical approach in the untact age},
Journal = {JOURNAL OF EXERCISE REHABILITATION},
Year = {2021},
Volume = {17},
Number = {4},
Pages = {222-225},
Month = {AUG},
Abstract = {Flipped learning, abbreviated as ``FL{''} is a new pedagogical approach
   that reverses the order of a typical lecture. It was developed to
   compensate for the shortcomings of a typical teaching method. Students
   learn by watching video clips before class and continue to learn through
   peer discussion in the classroom. The educational benefits of FL like
   student's positive attitude were revealed in the previous studies. FL
   can be a valuable alternative to the existing classes in the untact age
