@article{ WOS:000524988100006,
Author = {Furse, Cynthia M. and Ziegenfuss, Donna Harp},
Title = {A Busy Professor's Guide to Sanely Flipping Your Classroom: Bringing
   active learning to your teaching practice},
Journal = {IEEE ANTENNAS AND PROPAGATION MAGAZINE},
Year = {2020},
Volume = {62},
Number = {2},
Pages = {31-42},
Month = {APR},
Abstract = {The ``flipped classroom{''} has become a popular teaching method.
   Students watch video lectures before class, saving time for active
   learning (problem solving, demonstrations, applications, and so forth).
   This article provides a useful guide for busy professors who would like
   to try the flipped-classroom approach. Recommendations for four teaching
   components are given: 1) planning the flipped classroom, 2) video
   lectures, 3) active learning, and 4) student-centered formative
   assessment. It includes suggestions for getting started with the
   flipped-teaching method, case studies from electromagnetics (EM), and
   feedback from students about the resources they find most useful, their
   comfort level with asking questions, and their opinions of the flipped
   class during the semester. Training materials for flipping your
   classroom (or helping others do so) are available at
   http://teach-flip.utah.edu.},
