Title = {Teaching During the COVID-19 Pandemic: The Experience of the Faculty of
   Medicine at the Technion Israel Institute of Technology},
Journal = {ISRAEL MEDICAL ASSOCIATION JOURNAL},
Year = {2021},
Volume = {23},
