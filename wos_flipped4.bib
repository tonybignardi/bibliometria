@article{ WOS:000656772000009,
Author = {Ramasamy, S. and Pavithran, Varsha K. and Anjalin, J. Reni},
Title = {An insight on innovative teaching strategies: A cross-sectional survey
   among the oral medicine and radiology faculties in Tamil Nadu and
   Pondicherry, India},
Journal = {JOURNAL OF INDIAN ACADEMY OF ORAL MEDICINE AND RADIOLOGY},
