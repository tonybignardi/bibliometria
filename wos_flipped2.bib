@article{ WOS:000467647500007,
Author = {Zhao, Yu and Morris, Anne and Marais, Ben J. and Pardo, Abelardo and
   Scott, Karen M.},
Title = {Exploring how medical students learn during clinical rotations: a pilot
   study with a mobile application},
Journal = {HEALTH AND TECHNOLOGY},
