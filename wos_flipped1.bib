@article{ WOS:000452572900005,
Author = {Palazon-Herrera, Jose},
Title = {Audiovisual resources for instrumental practice in a flipped classroom
   setting},
Journal = {REVISTA ELECTRONICA DE LEEME},
Year = {2018},
Number = {42},
Pages = {54-69},
Abstract = {This article presents a classroom experience in which Secondary School
   music students have worked on modern music themes under flipped
   classroom methodology using videos as a fundamental resource for
   instrumental practice. The results of a descriptive analysis show that
   students have a very positive assessment both of the methodology used
   (tasks in class vs explanations of the teacher at home; adaptation to
   the student's rhythm; interest in instrumental practice; better
   knowledge for group work) and of the videos designed (contents similar
   to an expository class; increase of confidence for face-to-face classes;
   motivation of use; duration; quality and independence of students with
   respect to their teacher). Although it is an exploratory study, it can
   be affirmed that the design of videos and their use under an adequate
   methodology can have a positive effect on instrumental practice inside
   and outside the classroom.},
