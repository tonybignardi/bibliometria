@article{ WOS:000561739700010,
Author = {Tsai, Hsin-Luen and Wu, Jia-Fen},
Title = {Bibliometric analysis of flipped classroom publications from the Web of
   Science Core Collection published from 2000 to 2019},
Journal = {SCIENCE EDITING},
