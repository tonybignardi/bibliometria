@article{ WOS:000604840300001,
Author = {Debbag, Murat and Yildiz, Sevilay},
Title = {Effect of the flipped classroom model on academic achievement and
   motivation in teacher education},
Journal = {EDUCATION AND INFORMATION TECHNOLOGIES},
