@article{ WOS:000456133500008,
Author = {Sedraz Silva, Joao Carlos and Zambom, Erik and Rodrigues, Rodrigo Lins
   and Cavalcanti Ramos, Jorge Luis and de Souza, Fernando da Fonseca},
Title = {Effects of Learning Analytics on Students' Self-Regulated Learning in
   Flipped Classroom},
