@article{ WOS:000475750000006,
Author = {Shibukawa, Sachika and Taguchi, Mana},
Title = {Exploring the difficulty on students' preparation and the effective
   instruction in the flipped classroom: A case study in a physiology class},
Journal = {JOURNAL OF COMPUTING IN HIGHER EDUCATION},
Year = {2019},
Volume = {31},
