@article{ WOS:000429868600001,
Author = {Yilmaz, Fatma Gizem Karaoglan and Olpak, Yusuf Ziya and Yilmaz, Ramazan},
Title = {The Effect of the Metacognitive Support via Pedagogical Agent on
   Self-Regulation Skills},
