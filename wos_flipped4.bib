@article{ WOS:000459616400006,
Author = {Nagendrababu, V. and Pulikkotil, S. J. and Sultan, O. S. and Jayaraman,
   J. and Soh, J. A. and Dummer, P. M. H.},
Title = {Effectiveness of technology-enhanced learning in Endodontic education: a
   systematic review and meta-analysis},
Journal = {INTERNATIONAL ENDODONTIC JOURNAL},
Year = {2019},
Volume = {52},
Number = {2},
Pages = {181-192},
Month = {FEB},
Abstract = {The aim of the present systematic review was to evaluate the
   effectiveness of technology-enhanced learning (TEL) in the field of
   Endodontics to improve educational outcomes compared to traditional
   learning methods. Randomized controlled studies published in English
   were identified from two electronic databases (PubMed and Scopus) up to
   May 2018. Two authors independently performed study selection, data
   extraction and assessed the risk of bias (ROB). Any teaching method
   using TEL was considered as the intervention, and this was compared to
   traditional methods. The outcome measuring the effectiveness of learning
   activities was evaluated by Kirkpatrick's four-level training evaluation
   model. The four levels of training outcomes are as follows: Reaction,
   Learning, Behaviour and Results. A meta-analysis was performed to
   estimate the standardized mean difference (SMD) by the random effects
   model. In total, 13 studies were included in the systematic review. Only
