@article{ WOS:000471798400012,
Author = {Goh, Choon Fu and Ong, Eng Tek},
Title = {Flipped classroom as an effective approach in enhancing student learning
   of a pharmacy course with a historically low student pass rate},
Journal = {CURRENTS IN PHARMACY TEACHING AND LEARNING},
Year = {2019},
