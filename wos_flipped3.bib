@article{ WOS:000475667400004,
Author = {Lin, Chi-Jen},
Title = {An online peer assessment approach to supporting mind-mapping flipped
   learning activities for college English writing courses},
Journal = {JOURNAL OF COMPUTERS IN EDUCATION},
Year = {2019},
