@article{ WOS:000516813800002,
Author = {Solimine, Giovanni and Marzal Garcia-Quismondo, Miguel Angel},
Title = {Proposal of Visual Literacy Indicators for competencies courses. An
   academic literacy perspective for academic excellence},
