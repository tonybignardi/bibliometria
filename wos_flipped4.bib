@article{ WOS:000578432600001,
Author = {Hwang, Gwo-Jen and Chang, Shao-Chen and Song, Yanjie and Hsieh,
   Min-Chuan},
Title = {Powering up flipped learning: An online learning environment with a
   concept map-guided problem-posing strategy},
Journal = {JOURNAL OF COMPUTER ASSISTED LEARNING},
