@article{ WOS:000642018600001,
Author = {Kuba, Renata and Rahimi, Seyedahmad and Smith, Ginny and Shute, Valerie
   and Dai, Chih-Pu},
Title = {Using the first principles of instruction and multimedia learning
   principles to design and develop in-game learning support videos},
