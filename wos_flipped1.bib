@article{ WOS:000658256400003,
Author = {Zhang, Xiangfeng and Wang, Yanmei and Sharma, Shalini},
Title = {Design and analysis of flipped classroom experiment teaching based on
   DBR in information technology environment},
Journal = {INTERNATIONAL JOURNAL OF SYSTEM ASSURANCE ENGINEERING AND MANAGEMENT},
Abstract = {The flipped classroom concept is one of the latest advents in the
   education technology that builds up the standard for teaching-learning
   process fostering the students in higher education learning. In order to
   study the flipped classroom experimental teaching based on DBR in the
   information technology environment, this paper based on the DBR
   education research paradigm, through the practical exploration in the
   real situation to verify the original concept. A flipped classroom
   experimental teaching mode is designed based on implementation and
   verification, improving perfect the mode, and finally constructing the
   flipped classroom experimental teaching mode suitable for experimental
   teaching. On the basis of comprehensive analysis, the flipped classroom
   experimental teaching mode v1.0 is improved and perfected, and a more
   efficient flipped classroom experimental teaching mode v2.0 is
   constructed. The outcomes obtained provides an average score of 91.89\%
   and 82.36\% for the experimental class 6 and the parallel class 12
   respectively. The research paradigm has the characteristics of
   application and knowledge seeking, which has attracted great attention
   of the international education circle. Its application has been involved
   in many fields such as pedagogy, psychology, educational technology and
   so on.},
Publisher = {SPRINGER INDIA},
Address = {7TH FLOOR, VIJAYA BUILDING, 17, BARAKHAMBA ROAD, NEW DELHI, 110 001,
   INDIA},
Type = {Article; Early Access},
