@article{ WOS:000569350000001,
Author = {Tang, Tao and Abuhmaid, Atef M. and Olaimat, Melad and Oudat, Dana M.
   and Aldhaeebi, Maged and Bamanger, Ebrahim},
Title = {Efficiency of flipped classroom with online-based teaching under
   COVID-19},
Journal = {INTERACTIVE LEARNING ENVIRONMENTS},
Abstract = {Due to the spread of COVID-19 worldwide, a large number of universities
   had to close their campuses. To maintain teaching and learning during
   this disruption to the traditional teaching, most universities have
   adopted online teaching model. The current study aimed at investigating
   the efficacy of various online teaching modes as well as comparing a
   proposed combined model of online and flipped learning to other online
   and traditional models. The Learning under COVID-19questionnaire was
   designed and administered to undergraduate engineering students at
   Chengdu University of Information Technology (CUIT). The questionnaire
   included five parts: demographic questions, frequencies of online
   courses, types of online courses, the communication and Q\&A in online
   classes and the effect of online classes, as well as the effect of
   combined model learning. The results of the study showed that, students
   were dissatisfied with online learning in general, and they were
   especially dissatisfied with the communication and Q\&A modes. In
   addition, the combined model of online teaching with the flipped
   learning improved students' learning, attention, and evaluation of
   courses.},
