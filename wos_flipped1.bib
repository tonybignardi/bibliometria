@article{ WOS:000508864500007,
Author = {Chan, Siok-Yee and Lam, Yat Kuan and Ng, Theam Foo},
Title = {Student's perception on initial experience of flipped classroom in
   pharmacy education: Are we ready?},
Journal = {INNOVATIONS IN EDUCATION AND TEACHING INTERNATIONAL},
Year = {2020},
Volume = {57},
Number = {1},
Pages = {62-73},
Month = {JAN 2},
Abstract = {In Malaysia, the new teaching model of a flipped classroom is emerging
   in higher education with the aim of promoting student-centred learning.
   Hence the main purpose of this study is to identify the students'
   acceptance level, their perceptions, and the impact of their initial
   exposures to the flipped classroom model. With this in mind, a survey on
   undergraduate students' perceptions of the flipped classroom was
   evaluated at the School of Pharmaceutical Sciences, Universiti Sains
   Malaysia. Surprisingly, there was a low degree of acceptance and
   unfavourable impressions of flipped classrooms among students. The
   reasons were identified to be the inappropriate use of the flipped
   methodology and the increase of student workload. In conclusion, the
   implementation of the flipped classroom method was not straightforward
   in Malaysia with a tradition of more didactic teaching methods.},
