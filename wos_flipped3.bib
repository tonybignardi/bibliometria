@article{ WOS:000473926700004,
Author = {Diniz, P. C. A. and Veiga, A. C. P. and Rocha, G. N.},
Title = {Adapting team-based learning for application in telecommunications
   engineering using software-defined radio},
Journal = {INTERNATIONAL JOURNAL OF ELECTRICAL ENGINEERING EDUCATION},
Year = {2019},
Volume = {56},
Number = {3},
Pages = {238-250},
Month = {JUL},
Abstract = {One of the great challenges of engineering education is bringing theory
   closer to practical applications. The team-based learning methodology,
   which uses the flipped classroom concept, is actively centred on the
   student. The students study a conceptual material independently before
   it is covered in the classroom. After that, they spend a considerable
   amount of time working in groups in the classroom solving problems with
   conceptual application of the material studied. The last stage of
   team-based learning is the application of course concepts. However, in
   the telecommunications field, the high cost of equipment for practical
   development in educational institutions is a big barrier. Moreover, the
   equipment used is traditionally dedicated to a single activity. In order
   to solve this problem and apply all the team-based learning stages in
   telecommunications engineering teaching, this paper presents a proposal
   of an adaptation of this methodology using a software-defined radio set
   using the universal software radio peripheral hardware and the GNU Radio
   software. This set offers flexible functionalities and low cost.
   Finally, we show the proposal for each team-based learning stage in the
   learning process of the amplitude modulator and demodulator - double
   sideband suppressed carrier relating concepts to practical applications.},
