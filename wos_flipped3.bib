@article{ WOS:000516699700001,
Author = {Motley, Elizabeth},
Title = {Building a Thriving Lifestyle Medicine Practice Within a Primary Care
   Clinic: A Model for Aspiring Lifestyle Medicine Practitioners},
Journal = {AMERICAN JOURNAL OF LIFESTYLE MEDICINE},
Year = {2020},
Volume = {14},
Number = {2},
Pages = {133-136},
Month = {MAR},
Abstract = {The field of Lifestyle Medicine (LM) is growing rapidly as individuals
   and communities seek real solutions to the hardship of chronic disease.
   Providers across various medical specialties and allied health
   professions are gaining certification in this field, and yet are
   struggling with implementation. Time constraints, concern about
   reimbursement, and lack of clinical experience in counseling patients
   are often cited as obstacles. This article will address these issues and
   demonstrate how LM can be successfully practiced in a standard primary
   care setting. Active participation in the local community is essential.
   Office visits must be prepared for efficiently, and encounters should
   focus on teaching simple concepts to patients. At-home learning
   activities can then be used to build on these concepts. Referral to
   health coaches and outside programs are helpful options as well.
   Equipping providers with practice tools to promote lifestyle change will
   improve satisfaction for patients and providers alike and is an
   important step toward advancing the field of LM.},
