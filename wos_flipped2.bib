@article{ WOS:000615146800029,
Author = {Bock, A. and Heitzer, M. and Lemos, M. and Peters, F. and Elvers, D. and
   Kniha, K. and Hoelzle, F. and Modabber, A.},
Title = {``Flipped OR{''}: a modified didactical concept for a surgical clerkship
   in Oral and Maxillofacial Surgery},
Journal = {BRITISH JOURNAL OF ORAL \& MAXILLOFACIAL SURGERY},
Year = {2020},
Volume = {58},
