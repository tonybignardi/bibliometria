@article{ WOS:000435587100030,
Author = {Javier Hinojo-Lucena, Francisco and Custodio Mingorance-Estrada, Angel
   and Manuel Trujillo-Torres, Juan and Aznar-Diaz, Inmaculada and Caceres
   Reche, Maria Pilar},
Title = {Incidence of the Flipped Classroom in the Physical Education Students'
   Academic Performance in University Contexts},
Journal = {SUSTAINABILITY},
Year = {2018},
Volume = {10},
Number = {5},
Month = {MAY},
Abstract = {This research analyzed Physical Education students' degree of academic
   performance with the incorporation of active methodologies, specifically
   flipped classroom mixed learning, restricted to evaluation periods in
   the months of June and September. The study focused on whether there are
   significant differences in this variable through the scores obtained.
   Through a simple random sampling, 131 students participated in this
   empiric-analytic research, using an ex-post-facto study with a
   retrospective design with quasi-control group. A robust test of averages
   comparison, multiple linear regressions and an evaluation of the
   relative importance of predictors was conducted. The results show how
   flipped classroom methodology linearly and positively influences
   academic performance and correlational motivation and support. As main
   conclusion, in a hybrid and digitalized learning context, the value of
   the consideration of active methodologies (flipped classroom) based on
   emerging pedagogies, allows improving students' achievement and
   competence development, providing critical, significant, ubiquitous,
   transformational and especially motivating experiences.},
