@article{ WOS:000487167200011,
Author = {Siegel, Sharon C. and Kramer, Steven B. and Deranek, Kimberly M.},
Title = {Lean Methods Applied to CAD/CAM Pedagogy in the Dental Simulation
   Laboratory},
