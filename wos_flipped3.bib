@article{ WOS:000753525300002,
Author = {Knowles, Nikolas K. and DeCoito, Isha},
Title = {Biomedical engineering undergraduate education: A Canadian perspective},
Journal = {INTERNATIONAL JOURNAL OF MECHANICAL ENGINEERING EDUCATION},
Year = {2020},
Volume = {48},
