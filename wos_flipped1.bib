@article{ WOS:000522962200007,
Author = {Zheng, Binbin and Zhang, Yining},
Title = {Self-regulated learning: the effect on medical student learning outcomes
   in a flipped classroom environment},
Journal = {BMC MEDICAL EDUCATION},
Year = {2020},
Volume = {20},
Number = {1},
Month = {MAR 31},
Abstract = {BackgroundThe flipped-classroom model is increasingly being adopted in
   competency-based medical education. However, it poses a major challenge
   to students who have not mastered self-regulated learning strategies.
   This study explores which self-regulated learning skills affect student
   learning performance in the first 2 years of medical school at a
   university in the midwestern United States.MethodsSurvey data were used
   to assess how 146 first- and second-year medical students' use of
   self-regulated learning strategies affected their performance on
   standardized tests.ResultsBased on the results of regression analysis
   and content analysis, it was found that the use of peer learning and
   help-seeking positively affected the performance of first- and
   second-year students, respectively; whereas the use of rehearsal had a
   negative effect on student learning outcomes.ConclusionsThe study
   findings imply that during the transition period from traditional
   lecture-intensive learning to flipped-classroom learning, promoting peer
   learning and help-seeking could significantly improve students' academic
   achievement.},
