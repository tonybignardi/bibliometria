@article{ WOS:000476896800005,
Author = {Collins, Benjamin V. C.},
Title = {Flipping the precalculus classroom},
Journal = {INTERNATIONAL JOURNAL OF MATHEMATICAL EDUCATION IN SCIENCE AND
   TECHNOLOGY},
Year = {2019},
Volume = {50},
