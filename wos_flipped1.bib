@article{ WOS:000502129900012,
Author = {Wilson, Jennifer A. and Waghel, Rashi C. and Dinkins, Melissa M.},
Title = {Flipped classroom versus a didactic method with active learning in a
   modified team-based learning self-care pharmacotherapy course},
Journal = {CURRENTS IN PHARMACY TEACHING AND LEARNING},
