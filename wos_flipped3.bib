@article{ WOS:000744234600001,
Author = {Kattner, Aila Akosua},
Title = {Greek gods and the double-edged sword of liver regeneration},
Journal = {BIOMEDICAL JOURNAL},
Year = {2021},
Volume = {44},
Number = {5},
Pages = {515-520},
Month = {OCT},
Abstract = {In the current issue of the Biomedical Journal we gain an insight into
   the regeneration of liver tissue and how an over-the-counter supplement,
   stem cells and two plant extracts counteract liver damage. Furthermore
   the advances against hepatitis C virus are presented, the role of long
   non-coding RNA elucidated as well as the potential of an adhesion G
   protein-coupled receptor. In another contribution, the definition and
   evolutionary impact of copy number variants is clarified. Also, the
   polymorphism of a scaffolding caspase is investigated. We furthermore
   learn about the relation between SARS-CoV2 mutants in dependence of
   geography and explore the challenges of telemedicine in a complex
   healthcare field. A novel approach to engineering artificial grafts is
   presented, the challenges of total knee arthroplasty discussed as well
   as a possible mean of sinus floor elevation for dental implants. At last
   the concept of flipped classroom is scrutinized in terms of usefulness
   for a hospital in Taiwan.},
