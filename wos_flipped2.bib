@article{ WOS:000425326100025,
Author = {Leite, F. N. and Hoji, E. S. and Abdalla Junior, H.},
Title = {A Blended Learning Method Applied in Data Communication and Computer
   Networks Subject},
Journal = {IEEE LATIN AMERICA TRANSACTIONS},
