@article{ WOS:000759543700017,
Author = {Pires, Jose Luis and Payo, Pablo and Marcos, Ricardo},
Title = {The use of simulators for teaching fine needle puncture in veterinary
   medicine},
Journal = {JOURNAL OF VETERINARY MEDICAL EDUCATION},
Year = {2022},
Volume = {49},
Number = {1},
Pages = {138-143},
Month = {FEB 1},
Abstract = {Fine needle puncture (FNP) is widely used by veterinarians and is mostly
   taught by observation. Currently, the use of simulators is considered to
   improve the learning of practical procedures. However, FNP simulators
   have not yet been evaluated in veterinary medicine. Fifty-one veterinary
   students with no previous experience in cytology were randomly assigned
   to two groups that used a simulator (box with artificial nodules) and a
   piece of fruit (banana). A flipped classroom design with stations was
   used: first, students watched a tutorial video on FNP, then used the
   simulator or fruit piece for up to 15 minutes. Then, students performed
   the procedure on a realistic animal model, and a objective structured
   clinical examination (OSCE) was performed. Learning through the models
   was compared using quizzes, OSCE pass rate, and quality of the smears
   obtained. After watching the video tutorial, no students expressed being
   able to perform the FNP on a live animal. Conversely, most revealed that
   they were capable after the simulation. Students practiced longer on the
   box (14.8 +/- 0.8 min) than on the piece of fruit (8.5 +/- 2.2 min). On
   evaluation, the former had greater accuracy on the FNP. Still, there
   were no differences in the pass rate in OSCE. Therefore, both models are
   effective for learning the FNP, but the box has advantages regarding
   autonomous practice repetition. This repetition seems to have positive
   effects on FNP accuracy, which has relevance from a clinical point of
   view.},
