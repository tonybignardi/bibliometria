@article{ WOS:000526413900005,
Author = {Atkinson, Molly B. and Krishnan, Sandhya and McNeil, LaShawn A. and
   Luft, Julie A. and Pienta, Norbert},
Title = {Constructing Explanations in an Active Learning Preparatory Chemistry
   Course},
Journal = {JOURNAL OF CHEMICAL EDUCATION},
Year = {2020},
