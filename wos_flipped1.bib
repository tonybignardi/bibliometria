@article{ WOS:000490736400001,
Author = {Chiquito, Maria and Castedo, Ricardo and Santos, Anastasio P. and Lopez,
   Lina M. and Alarcon, Covadonga},
Title = {Flipped classroom in engineering: The influence of gender},
