@article{ WOS:000595659900007,
Author = {Cui, Tristan and Coleman, Andrew},
Title = {Investigating Students' Attitudes, Motives, Participation and
   Performance Regarding Out-of-Class Communication (OCC) in a Flipped
   Classroom},
Journal = {ELECTRONIC JOURNAL OF E-LEARNING},
Year = {2020},
