@article{ WOS:000520961200001,
Author = {Staddon, V, Rachel},
Title = {Bringing technology to the mature classroom: age differences in use and
   attitudes},
Journal = {INTERNATIONAL JOURNAL OF EDUCATIONAL TECHNOLOGY IN HIGHER EDUCATION},
Year = {2020},
Volume = {17},
Number = {1},
Month = {MAR 23},
Abstract = {Mature students are anecdotally thought to be more anxious about
   technology than younger students, to the extent that they avoid using
   technology. This is a problem in today's higher education classrooms
   which often use a range of learning technologies, particularly as
   cohorts are becoming more and more likely to contain mature students.
   Previous work examining the attitudes of mature students to technology
   no longer reflects contemporary student age profiles or the current
   technological landscape. This study asks whether modern mature students
   in a UK university have more negative attitudes towards technology than
   younger students, and whether their usage of technology is different. A
   new diagnostic instrument, the Technology Attitudes Questionnaire, was
   developed to determine how students use technology for course activities
   and personal use, and their attitudes towards technology more generally.
   It was found that mature students use fewer technologies than younger
   students and use them less frequently, but have used them for a longer
   period over their lives. No difference was found for attitudes towards
   technology between the mature and younger groups. This research aims to
   contribute to the wider field of technology attitudes and use,
   particularly for the modern mature student cohort. These findings can be
   used to inform how educators design learning resources and use
   technology on their courses, working towards an age-inclusive programme.},
