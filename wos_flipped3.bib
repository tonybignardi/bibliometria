@article{ WOS:000431536700005,
Author = {Kim, Yeon and Ahn, Changsun},
Title = {Effect of Combined Use of Flipped Learning and Inquiry-Based Learning on
   a System Modeling and Control Course},
Journal = {IEEE TRANSACTIONS ON EDUCATION},
